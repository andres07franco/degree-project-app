     documento_A.`documento_id` AS documento_A_documento_id,
     tercero.`nombre` AS tercero_nombre,
     documento.`estado` AS documento_estado,
     tipo_documento.`descripcion` AS tipo_documento_descripcion
FROM
     `documento` documento INNER JOIN `documento` documento_A ON documento.`id` = documento_A.`documento_id`
     INNER JOIN `tercero` tercero ON documento_A.`tercero` = tercero.`id`
     INNER JOIN `tipo_documento` tipo_documento ON documento_A.`tipo` = tipo_documento.`id`
